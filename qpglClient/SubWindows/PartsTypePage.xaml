<Page x:Class="qpglClient.SubWindows.PartsTypePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:qpglClient.SubWindows"
      xmlns:userControls="clr-namespace:qpglClient.UserControls"
      mc:Ignorable="d" 
      d:DesignHeight="300"
      Title="PartsTypePage" Height="Auto" MinWidth="750" MinHeight="500" Background="White">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="32*"/>
            <ColumnDefinition Width="43*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="19*"/>
        </Grid.RowDefinitions>
        <userControls:PageToolsMenu d:LayoutOverrides="LeftMargin, RightMargin, TopMargin, BottomMargin"/>
        <StackPanel Grid.Row="1" Grid.ColumnSpan="2">
            <Grid Height="25" Background="#FFE0E0C9">
                <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Width="235"/>
            </Grid>
        </StackPanel>

    </Grid>
</Page>
